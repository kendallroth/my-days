{
  "cli": {
    "version": ">= 0.41.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_VARIANT": "development"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preview"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "node": "16.6.0",
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
